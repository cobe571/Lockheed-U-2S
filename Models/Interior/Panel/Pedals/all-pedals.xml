<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <!-- Left pedal -->
  <model>
    <name>PedalL</name>
    <path>Aircraft/Lockheed-U-2S/Models/Interior/Panel/Pedals/pedal.xml</path>
    <offsets>
      <x-m> -6.41148 </x-m>
      <y-m> -0.16235 </y-m>
      <z-m>  1.16675 </z-m>
    </offsets>
  </model>

  <!-- Right pedal -->
  <model>
    <name>PedalR</name>
    <path>Aircraft/Lockheed-U-2S/Models/Interior/Panel/Pedals/pedal.xml</path>
    <offsets>
      <x-m> -6.41148 </x-m>
      <y-m>  0.16235 </y-m>
      <z-m>  1.16675 </z-m>
    </offsets>
  </model>
 
  <animation>
    <type>translate</type>
    <object-name>PedalL</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep> -0.1 </dep></entry>
      <entry><ind>  0 </ind><dep>  0.0 </dep></entry>
      <entry><ind>  1 </ind><dep>  0.1 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>PedalR</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep>  0.1 </dep></entry>
      <entry><ind>  0 </ind><dep>  0.0 </dep></entry>
      <entry><ind>  1 </ind><dep> -0.1 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
